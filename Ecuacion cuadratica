// ResolucionEcuacionCuadratica.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include <math.h>
using namespace std;
void main()
{
	float a,b,c,x1,x2;
	cout<<"Ingrese el valor de a: ";
	cin>>a;
	cout<<"Ingrese el valor de b: ";
	cin>>b;
	cout<<"Ingrese el valor de c: ";
	cin>>c;
	x1=(-b+sqrt(b*b-4*a*c))/(2*a);
	x2=(-b-sqrt(b*b-4*a*c))/(2*a);
	cout<<"El valor de x1 sera: ";
	cout<<x1;
	cout<<"El valor de x2 sera: ";
	cout<<x2;
	getch();
}
