// jhgh.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
using namespace std;
void main ()
{
	int i,n,fact;
	fact=1;
	i=1;
	cout<<"Escriba el valor de n: ";
	cin>>n;
	while (i<=n)
	{ fact=fact*i;
	i=i+1;
	}
	cout<<"El resultado sera: "<<fact;
	getch ();
}

